{
    "Variables": {
        "mainColor": "red"
    },
    
    "Phone": {
        "UINavigationBar": {
            "tintColor": "gray"
        }
    },
    "Pad": {
        "UINavigationBar": {
            "backgroundImage": ["background.png", "default"]
        }
    },
    "UITabBar": {
        "tintColor": "darkGray",
        "selectedImageTintColor": "$mainColor",
        "UITabBarItem": {
            "titleTextAttributes": [[{"textColor": "red"}, "selected"],
                                    [{"textColor": "white"}, "normal"]]
        }
    },
    "UIToolbar": {
        "tintColor": "lightGray",
        "backgroundImage": ["background", "bottom"]
    },
    "UISSCustomView": {
        "-backgroundImage": "background.png",
        "image": "background.png",
        "backgroundImage": null,
        "backgroundColor": "gray",
        "backgroundMargin": 20,
        "UIImageView": {
            "backgroundColor": "red"
        }
    },
    "UILabel": {
        "Phone": {
            "textColor": "blue"
        },
        "Pad": {
            "textColor": "orange"
        }
    },
    "UISSCustomContainerView": {
        "backgroundColor": [["#8899ff", 0.5]]
    },
    "UISSDemoSecondViewController": {
        "UILabel": {
            "textColor": "white",
            "backgroundColor": "black"
        },
        "UISSCustomContainerView": {
            "UILabel": {
                "backgroundColor": "white",
                "textColor": "black"
            }
        }
    },
    "UISSDemoFirstViewController": {
        "UIButton": {
            "titleColor": [["black", "normal"], ["white", "highlighted"]],
            "backgroundImage": ["background", "highlighted"],
        },
        "UILabel": {
            "textColor": "blue",
            "backgroundColor": "lightGray"
        }
    }
}