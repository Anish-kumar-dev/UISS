{
    "Variables": {
        "tabbarColor": "darkGray"
    },
    "Phone": {
        "UINavigationBar": {
            "tintColor": "gray"
        }
    },
    "Pad": {
        "UINavigationBar": {
            "backgroundImage": ["background.png", "default"]
        }
    },

    "UITabBar": {
        "tintColor": "$tabbarColor",
        "selectedImageTintColor": "blue",
        "UITabBarItem": {
            "titleTextAttributes": [[{"textColor": "white"}, "selected"],
                                    [{"textColor": "lightGray"}, "normal"]]
        },
        "UILabel": {
            "backgroundColor": "clear"
        }
    },
    "UIToolbar": {
        "tintColor": "blue",
        "backgroundImage": ["background", "bottom", "default"]
    },
    "UISSCustomView": {
        "debugBorderColor": "red",
        "image": "background.png",
        "backgroundColor": "gray",
        "backgroundMargin": 20,
        "UIImageView": {
            "backgroundColor": [["lightGray", 0.2]]
        }
    },
    "UILabel": {
        "Phone": {
            "textColor": "blue"
        },
        "Pad": {
            "textColor": "orange",
            "backgroundColor": "green"
        }
    },
    "UISSCustomContainerView": {
        "backgroundColor": [["#8899ff", 0.5]]
    },
    "@Controllers": {
        "UISSDemoSecondViewController": {
            "UILabel": {
                "textColor": "white",
                "backgroundColor": "black"
            },
            "UISSCustomContainerView": {
                "debugBorderColor": "black",
                "UILabel": {
                    "backgroundColor": "white",
                    "textColor": "black"
                }
            }
        },
        "UISSDemoFirstViewController": {
            "UIButton": {
                "titleColor": [["black", "normal"], ["white", "highlighted"]],
                "backgroundImage": [["background", "highlighted"], ["blue", "normal"]],
                "UILabel": {
                    "backgroundColor": "clear"
                }
            },
            "UILabel": {
                "textColor": "white",
                "backgroundColor": "darkGray"
            }
        }
    }
}