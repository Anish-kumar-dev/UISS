{
    "Variables": {
        "tintColor": "red",
        "backgroundColor": ["#770077", 0.5],
        "insets": [100, 100, 100, 100]
    },
    "Phone": {
        "UINavigationBar": {
            "backgroundImage": ["background.png", "default"]
        }
    },
    "Pad": {
        "UINavigationBar": {
            "tintColor": "$tintColor"
        }
    },
    "UIToolbar": {
        "tintColor": "greenColor",
        "backgroundImage": ["background", "bottom"]
    },
    "UISSCustomView": {
        "backgroundImage": "background.png"
    },
    "UILabel": {
        "Phone": {
            "textColor": "blue"
        },
        "Pad": {
            "textColor": "red"
        }
    },
    "UISSCustomContainerView": {
        "backgroundColor": ["$backgroundColor"]
    },
    "UISSDemoSecondViewController": {
        "UILabel": {
            "textColor": "orange",
            "backgroundColor": "black"
        },
        "UISSCustomContainerView": {
            "UILabel": {
                "backgroundColor": "white",
                "textColor": "blue"
            }
        }
    },
    "UISSDemoFirstViewController": {
        "UIButton": {
            "titleColor": [["green", "normal"], ["yellow", "highlighted"]],
            "backgroundImage": ["background", "highlighted"],
            "contentEdgeInsets": ["$insets"]
        },
        "UILabel": {
            "textColor": "orange"
        }
    }
}